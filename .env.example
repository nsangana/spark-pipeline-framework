# Environment variables for Spark Pipeline Framework
# Copy this file to .env and update with your values

# Data paths
DATA_PATH=/path/to/input/data
OUTPUT_PATH=/path/to/output/data

# Spark configuration (optional overrides)
SPARK_MASTER=local[*]
SPARK_APP_NAME=SparkPipeline

# Logging
LOG_LEVEL=INFO
LOG_FILE=

# Delta Lake settings (optional)
DELTA_RETENTION_HOURS=168
